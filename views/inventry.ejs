<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="/inventry.css">
    <title>My Inventory</title>
</head>
<body>
    <h1>My Inventory</h1>

    <% for (const genreGroup of books) { 
         const genre = Object.keys(genreGroup)[0];
         const genreBooks = genreGroup[genre];
    %>
        <h2><%= genre %></h2>
            <%- include("panel", { books: genreBooks }) %>
      
    <% } %>

   
</body>
</html>
